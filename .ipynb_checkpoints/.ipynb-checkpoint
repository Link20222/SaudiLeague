    "#since I am on the free plan on football-api, I am limited to 100 requests/day & 30 requests/minute.\n",
    "\n",
    "#The following process was \n",
    "\n",
    "for i in range(240):\n",
    "    \n",
    "    match = season2018.loc[i]\n",
    "    fixture_id = match[\"fixture_id\"]\n",
    "    \n",
    "    url = f\"https://api-football-v1.p.rapidapi.com/v2/lineups/{fixture_id}\"\n",
    "\n",
    "    headers = {\n",
    "        'x-rapidapi-host': \"api-football-v1.p.rapidapi.com\",\n",
    "        'x-rapidapi-key': \"d110139f75msh29b250f2354d68cp1f487ajsn3748c0ad1dca\"\n",
    "        }\n",
    "\n",
    "    response = requests.request(\"GET\", url, headers=headers).json()\n",
    "\n",
    "    #home team data\n",
    "    \n",
    "    home_coach = response[\"api\"][\"lineUps\"][match[\"home_team\"]][\"coach\"]\n",
    "    home_formation = response[\"api\"][\"lineUps\"][match[\"home_team\"]][\"formation\"]\n",
    "    \n",
    "    home_players = []\n",
    "    for player in response[\"api\"][\"lineUps\"][match[\"home_team\"]][\"startXI\"]:\n",
    "        home_players.append((player[\"player\"], player[\"pos\"]))\n",
    "\n",
    "\n",
    "    #away team data                                             \n",
    "    away_coach = response[\"api\"][\"lineUps\"][match[\"away_team\"]][\"coach\"]\n",
    "    away_formation = response[\"api\"][\"lineUps\"][match[\"away_team\"]][\"formation\"]\n",
    "    \n",
    "    away_players = []\n",
    "    for player in response[\"api\"][\"lineUps\"][match[\"away_team\"]][\"startXI\"]:\n",
    "        away_players.append((player[\"player\"], player[\"pos\"]))                                            \n",
    "            \n",
    "    season2018.loc[i, [\"home_coach\", \"away_coach\", \"home_formation\",\n",
    "                       \"away_formation\", \"home_players\",\n",
    "                       \"away_players\"]] = [home_coach, away_coach, home_formation,\n",
    "                       away_formation, home_players, away_players]\n",
    "    "
   ]
